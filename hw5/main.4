#include <iostream>

int main()
{
    int i=12345;
    int a=0;
    int b=0;
    int c=0;
    int d=0;
    int e=0;
    if (i>10000){
        a=i/10000;           //萬位數
        b=(i%10000)/1000;   //千位數
        c=((i%10000)%1000)/100; //百位
        d=(((i%10000)%1000)%100)/10; //十位
        e=(((i%10000)%1000)%100)%10;
        printf("%d%d%d%d%d",a,e,c,d,b);
    }
    else if(i>1000){
        a=i/1000;           
        b=(i%1000)/100;   
        c=((i%1000)%100)/10; 
        d=((i%1000)%100)%10; 
        printf("%d%d%d%d",d,b,c,a);
    }
    else if(i>100){
        a=i/100;           
        b=(i%100)/10;   
        c=((i%100)%10)%10; 
        printf("%d%d%d%d",c,b,a,0);
    }
    else if(i>10){
        a=i/10;
        b=i%10;
        printf("%d%d%d%d",b,0,a,0);
    }
    else{
        printf("%d%d%d%d",i,0,0,0);
    }
    

    return 0;
}
